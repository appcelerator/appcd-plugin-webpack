{
  "name": "@appcd/plugin-webpack",
  "version": "1.0.0",
  "description": "Manage Webpack builds with the Appc Daemon",
  "main": "./dist/index.js",
  "scripts": {
    "app:build": "cross-env NODE_OPTIONS=--max-old-space-size=4096 nuxt build",
    "app:dev": "cross-env NODE_ENV=development NODE_OPTIONS=--max-old-space-size=4096 nodemon app/server/index.js --watch app/server --watch app/nuxt.config.js",
    "app:lint": "eslint --ext .js,.vue --ignore-path .gitignore app",
    "build": "gulp build",
    "coverage": "gulp coverage",
    "dev": "gulp watch",
    "docs": "gulp docs",
    "prepare": "gulp build",
    "test": "gulp test"
  },
  "keywords": [
    "webpack",
    "appcd"
  ],
  "author": "Axway Appcelerator",
  "maintainers": [
    "Jan Vennemann <jvennemann@axway.com>"
  ],
  "license": "Apache-2.0",
  "homepage": "https://github.com/appcelerator/appc-daemon-plugins",
  "bugs": "https://github.com/appcelerator/appc-daemon-plugins/issues",
  "repository": "https://github.com/appcelerator/appc-daemon-plugins",
  "dependencies": {
    "@babel/core": "^7.6.0",
    "@hapi/joi": "^16.1.7",
    "@nuxtjs/axios": "^5.8.0",
    "@nuxtjs/proxy": "^1.3.3",
    "@soda/friendly-errors-webpack-plugin": "^1.7.1",
    "acorn": "^7.1.0",
    "acorn-walk": "^7.0.0",
    "alloy-loader": "^0.2.3",
    "alloy-utils": "^0.1.3",
    "appcd-fswatcher": "^1.2.3",
    "babel-loader": "^8.0.6",
    "cache-loader": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "cross-env": "^5.2.0",
    "hash-sum": "^2.0.0",
    "imports-loader": "^0.8.0",
    "koa": "^2.11.0",
    "loader-utils": "^1.2.3",
    "lodash.defaultsdeep": "^4.6.1",
    "lodash.mapvalues": "^4.6.0",
    "lodash.transform": "^4.6.0",
    "minimist": "^1.2.0",
